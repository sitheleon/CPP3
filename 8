#include <iostream>
using namespace std;

int main() {
    int a, b, cse;
    cout << "case: ";
    cin >> cse;
  

    for (int i = 0; i < cse; i++) {

        cout << "input a:";
        cin >> a;
        cout << "input b:";
        cin >> b;


        cout << "Case #" << i + 1 << ": " << a << " + " << b << " = " << a + b;
        cout << endl;
    }


    return 0;
}
